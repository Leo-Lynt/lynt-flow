{
  "version": 3,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://api-flow.lynt.io/api/$1"
    },
    {
      "src": "/editor/(.*)",
      "dest": "/editor/index.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
